\begin_layout Section*
6.7
\end_layout

\begin_layout Standard
Get all supplier-number/part-number/project-number triplies such that the
 indicated supplier, part, and project are not all colocated.
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

select  snum,pnum,jnum from S as s, P as p, J as j 
\end_layout

\begin_layout Plain Layout

where not (s.city=p.city and p.city=j.city);
\end_layout

\begin_layout Plain Layout

+------+------+------+
\end_layout

\begin_layout Plain Layout

| snum | pnum | jnum |
\end_layout

\begin_layout Plain Layout

+------+------+------+
\end_layout

\begin_layout Plain Layout

| S1   | P1   | J1   |
\end_layout

\begin_layout Plain Layout

| S2   | P1   | J1   |
\end_layout

\begin_layout Plain Layout

| S3   | P1   | J1   |
\end_layout

\begin_layout Plain Layout

| S4   | P1   | J1   |
\end_layout

\begin_layout Plain Layout

...output truncated
\end_layout

\begin_layout Plain Layout

