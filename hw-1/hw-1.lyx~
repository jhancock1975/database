\end_inset


\end_layout

\begin_layout Section*
6.8
\end_layout

\begin_layout Standard
Get all supplier-number/part-number/project-number triples such that no
 two of the indicated supplier, part, and project are not all colocated.
\end_layout

\begin_layout Standard
Our answer differs in form from the book answer but is equivalent.
\end_layout

\begin_layout Standard
Our answer:
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

select  snum,pnum,jnum from S as s, P as p, J as j where not (s.city=p.city
 or s.city=j.city or p.city=j.city);
\end_layout

\begin_layout Plain Layout

+------+------+------+
\end_layout

\begin_layout Plain Layout

| snum | pnum | jnum |
\end_layout

\begin_layout Plain Layout

+------+------+------+
\end_layout

\begin_layout Plain Layout

| S5   | P1   | J1   |
\end_layout

\begin_layout Plain Layout

| S1   | P3   | J1   |
\end_layout

\begin_layout Plain Layout

| S4   | P3   | J1   |
\end_layout

\begin_layout Plain Layout

| S5   | P3   | J1   |
\end_layout

\begin_layout Plain Layout

...
 output trucated ...
\end_layout

\begin_layout Plain Layout

79 rows in set (0.00 sec) 
\end_layout

\end_inset


\end_layout

\begin_layout Standard
Book answer:
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

select snum,pnum,jnum from S,P,J where S.city <> P.city and P.city <> J.city
 and J.city <> S.city;
\end_layout

\begin_layout Plain Layout

+------+------+------+
\end_layout

\begin_layout Plain Layout

| snum | pnum | jnum |
\end_layout

\begin_layout Plain Layout

+------+------+------+
\end_layout

\begin_layout Plain Layout

| S5   | P1   | J1   |
\end_layout

\begin_layout Plain Layout

| S1   | P3   | J1   |
\end_layout

\begin_layout Plain Layout

| S4   | P3   | J1   |
\end_layout

\begin_layout Plain Layout

| S5   | P3   | J1   |
\end_layout

\begin_layout Plain Layout

...output truncated...
\end_layout

\begin_layout Plain Layout

79 rows in set (0.00 sec) 
\end_layout

\end_inset


\end_layout

\begin_layout Section*
6.9
\end_layout

\begin_layout Standard
Get part numbers for parts supllied by a supplier in London.
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

select distinct spj.pnum 
\end_layout

\begin_layout Plain Layout

from S as s, SPJ as spj
\end_layout

\begin_layout Plain Layout

where spj.snum=s.snum and s.city='London';
\end_layout

\begin_layout Plain Layout

+------+
\end_layout

\begin_layout Plain Layout

| pnum |
\end_layout

\begin_layout Plain Layout

+------+
\end_layout

\begin_layout Plain Layout

| P1   |
\end_layout

\begin_layout Plain Layout

| P6   |
\end_layout

\begin_layout Plain Layout

+------+
\end_layout

\end_inset


\end_layout

\begin_layout Section*
6.10
\end_layout

\begin_layout Standard
Get part numbers for pars supplied by a supplier in London to a project
 in London.
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

select distinct spj.pnum 
\end_layout

\begin_layout Plain Layout

from SPJ as spj, S as s, J as j 
\end_layout

\begin_layout Plain Layout

where spj.snum=s.snum and spj.jnum=j.jnum 
\end_layout

\begin_layout Plain Layout

and s.city='London' and j.city='London';
\end_layout

\begin_layout Plain Layout

+------+
\end_layout

\begin_layout Plain Layout

| pnum |
\end_layout

\begin_layout Plain Layout

+------+
\end_layout

\begin_layout Plain Layout

| P6   |
\end_layout

\begin_layout Plain Layout

+------+
\end_layout

\end_inset


\end_layout

\begin_layout Section*
6.11
\end_layout

\begin_layout Standard
Get all pairs of city names such that a supplier in the first city supplies
 a project in the second city
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

select distinct s.city, j.city 
\end_layout

\begin_layout Plain Layout

from SPJ as spj, S as s, J as j
\end_layout

\begin_layout Plain Layout

where spj.snum=s.snum and spj.jnum=j.jnum;
\end_layout

\begin_layout Plain Layout

+--------+--------+
\end_layout

\begin_layout Plain Layout

| city   | city   |
\end_layout

\begin_layout Plain Layout

+--------+--------+
\end_layout

