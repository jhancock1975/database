\end_layout

\begin_layout Standard
Get all supplier-number/part-num/project-number triples such that the indicated
 supplier, part and project are all colocated.
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

select  snum,pnum,jnum from S as s, P as p, J as j
\end_layout

\begin_layout Plain Layout

where s.city=p.city and p.city=j.city;
\end_layout

