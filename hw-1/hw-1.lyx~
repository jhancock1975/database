\end_layout

\begin_layout Standard
Get a list of all part-color/part-city combinations, with duplicate color/city
 pairs eliminated.
\end_layout

\begin_layout Standard
\begin_inset listings
inline false
status open

\begin_layout Plain Layout

select distinct city, color from P;
\end_layout

\begin_layout Plain Layout

+--------+-------+
\end_layout

